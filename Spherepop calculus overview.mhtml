6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-racket"><span>#lang racket
(require racket/match "ast.rkt")
(provide eval run pp-dist)

;; Distribution as weighted list
(struct dist (outcomes) #:transparent)
;; outcome = (w . v)

(define (delta v) (dist (list (cons 1.0 v))))

(define (mix p d1 d2)
  (define (scale wvs s) (map (λ (wv) (cons (* s (car wv)) (cdr wv))) wvs))
  (dist (append (scale (dist-outcomes d1) p)
                (scale (dist-outcomes d2) (- 1 p)))))

;; Bind (monadic)
(define (bindD d f)
  (dist (for*/list ([wv (in-list (dist-outcomes d))]
                    [wv2 (in-list (dist-outcomes (f (cdr wv))))])
          (cons (* (car wv) (car wv2)) (cdr wv2)))))

;; Pretty print
(define (pp-dist d)
  (for ([wv (in-list (dist-outcomes d))])
    (printf "~a%  ⟼  ~a\n" (* 100 (car wv)) (cdr wv))))

;; Evaluation
(define (eval term env)
  (match term
    [(Var x) (delta (hash-ref env x (λ () (error "unbound" x))))]
    [(Sphere x ty body) (delta (VLam x body env))]
    [(Pop f u)
     (bindD (eval f env)
            (λ (funv)
              (bindD (eval u env)
                     (λ (arg)
                       (match funv
                         [(VLam x body clo)
                          (eval body (hash-set clo x arg))]
                         [else (error "Pop: not a function" funv)])))))]
    [(Merge t u)
     (bindD (eval t env)
            (λ (v)
              (bindD (eval u env)
                     (λ (w) (delta (VPair v w))))))]
    [(Choice p t u) (mix p (eval t env) (eval u env))]
    [(Rotate k t)
     (bindD (eval t env)
            (λ (v) (rotate-val k v)))]
    [(LitUnit) (delta (VUnit))]
    [(LitBool b) (delta (VBool b))]
    [(LitNat n) (delta (VNat n))]
    [(If b t u)
     (bindD (eval b env)
            (λ (bv)
              (match bv
                [(VBool #t) (eval t env)]
                [(VBool #f) (eval u env)]
                [else (error "If: not Bool")])))]
    [(Add a b)
     (bindD (eval a env)
            (λ (va)
              (bindD (eval b env)
                     (λ (vb)
                       (match (list va vb)
                         [(list (VNat x) (VNat y)) (delta (VNat (+ x y)))]
                         [else (error "Add: not Nat")])))))]))

;; Rotation helpers (homogeneous Bool tensor)
(define (flatten-pairs v)
  (match v
    [(VPair v1 v2) (append (flatten-pairs v1) (flatten-pairs v2))]
    [_ (list v)]))

(define (rebuild-right vs)
  (cond
    [(empty? vs) (error "empty rebuild")]
    [(= 1 (length vs)) (first vs)]
    [else (VPair (first vs) (rebuild-right (rest vs)))]))

(define (rotate-right k xs)
  (define n (length xs))
  (define r (modulo (+ k n) n))
  (append (drop xs (- n r)) (take xs (- n r))))

(define (all-bool-leaves v)
  (match v
    [(VBool _) #t]
    [(VPair v1 v2) (and (all-bool-leaves v1) (all-bool-leaves v2))]
    [_ #f]))

(define (rotate-val k v)
  (if (all-bool-leaves v)
      (delta (rebuild-right (rotate-right k (flatten-pairs v))))
      (error "Rotate: not Bool tensor")))

;; Runner
(define (run t)
  (pp-dist (eval t (hash))))
