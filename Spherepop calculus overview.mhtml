       [(TyArr a b)
        (if (equal? a tu)
            b
            (error (format "Type mismatch: expected ~a, got ~a" a tu)))]
       [_ (error "Attempted to Pop a non-function")])]
    [<code>(Merge ,t ,u) (TyTensor (type-of ctx t) (type-of ctx u))]
</code>    [<code>(Choice ,p ,t ,u)
</code>     (unless (&lt;= 0 p 1) (error "Choice probability must be in [0,1]"))
