#lang racket
; spherepop.rkt

(require racket/match)

; Types
(struct TyUnit () #:transparent)
(struct TyBool () #:transparent)
(struct TyNat () #:transparent)
(struct TyArr (dom cod) #:transparent)
(struct TyTensor (left right) #:transparent)

; Terms
(define (Var x) <code>(Var ,x))
</code>(define (Sphere x ty t) <code>(Sphere ,x ,ty ,t))
</code>(define (Pop f u) <code>(Pop ,f ,u))
</code>(define (Merge t u) <code>(Merge ,t ,u))
