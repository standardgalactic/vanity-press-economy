(provide (all-defined-out))
(require syntax/parse/define)

;; Surface forms:
;;   (sphere [x A] body)
;;   (pop f u)
;;   (merge t u)
;;   (choice p t u)
;;   (unit) (bool #t/#f) (nat n)
;;   (if b t u)
;;   (add a b)
;;   (rotate k t)

;; For a first pass, expand directly to an IR (here we just re-use same names).
;; In a fuller impl you'd separate syntax from runtime.

(define-syntax-parser sphere
  [(_ ([x A]) body) #'(Sphere 'x 'A body)])

(define-syntax-parser pop
  [(_ f u) #'(Pop f u)])

(define-syntax-parser merge
  [(_ t u) #'(Merge t u)])

(define-syntax-parser choice
  [(_ p t u) #'(Choice p t u)])

(define (unit) 'LitUnit)
(define (bool b) (if b '(LitBool #t) '(LitBool #f)))
(define (nat n)  `(LitNat ,n))
(define (if* b t u) `(If ,b ,t ,u))
(define (add a b) `(Add ,a ,b))
(define (rotate k t) `(Rotate ,k ,t))
</span></code></div></div></pre>
<h3 data-start="6956" data-end="6979">File: <code data-start="6966" data-end="6979">runtime.rkt</code></h3>
<p data-start="6981" data-end="7292">You can either re-implement the Haskell evaluator in Racket (straightforward), or—if this is just for <strong data-start="7083" data-end="7100">website demos</strong>—encode examples as JSON and render reductions client-side. For a quick REPLable prototype, mirror the evaluator structure from the Haskell code (maps, distributions, normalization) in Racket.</p>
