eval env = \case
  Var x -&gt; do
    v &lt;- liftEither (lookupVal env x)
    return v

  Sphere x _ t -&gt;
    return (VLam x t env)

  Pop f u -&gt; do
    vf &lt;- eval env f
    vu &lt;- eval env u
    case vf of
      VLam x body clo -&gt; eval (M.insert x vu clo) body
      other           -&gt; throwE $ "Tried to apply non-function: " ++ show other

  Merge t u -&gt; do
    vt &lt;- eval env t
    vu &lt;- eval env u
    return (VPair vt vu)

  Choice p t u -&gt; do
    vt &lt;- eval env t
    vu &lt;- eval env u
    ExceptT $ Dist [(p, Right vt), (1-p, Right vu)]

  Rotate k t -&gt; do
    v &lt;- eval env t
    if allBoolLeaves v
      then case rebuildRight (rotateRight k (flattenPairs v)) of
             Right r -&gt; return r
             Left e  -&gt; throwE e
      else throwE "Rotate: value is not a Bool tensor"

  If b t u -&gt; do
    vb &lt;- eval env b
    case vb of
      VBool True  -&gt; eval env t
      VBool False -&gt; eval env u
      other       -&gt; throwE $ "If: expected Bool, got " ++ show other

  Add a b -&gt; do
    va &lt;- eval env a
    vb &lt;- eval env b
    case (va,vb) of
      (VNat x, VNat y) -&gt; return (VNat (x+y))
