</span><span><span class="hljs-keyword">\Delta</span></span><span> </span><span><span class="hljs-keyword">\int</span></span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\mathcal</span></span><span>{V}} S </span><span><span class="hljs-keyword">\ge</span></span><span> </span><span><span class="hljs-keyword">\int</span></span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\partial</span></span><span><span class="hljs-keyword">\mathcal</span></span><span>{V}} </span><span><span class="hljs-keyword">\frac</span></span><span>{</span><span><span class="hljs-keyword">\delta</span></span><span> Q</span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\text</span></span><span>{eff}}}{T</span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\text</span></span><span>{eff}}}
