</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_page</span></span><span>(</span><span><span class="hljs-params">size=<span class="hljs-number">32</span></span></span><span>):
    </span><span><span class="hljs-string">"""Generate a synthetic 'page' with text-like patterns."""</span></span><span>
    page = np.zeros((size, size))
    </span><span><span class="hljs-comment"># horizontal and vertical bars</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> i </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, size, </span><span><span class="hljs-number">4</span></span><span>):
        page[i, :] = </span><span><span class="hljs-number">1</span></span><span>
        page[:, i] = </span><span><span class="hljs-number">1</span></span><span>
    </span><span><span class="hljs-comment"># add diagonal</span></span><span>
    np.fill_diagonal(page, </span><span><span class="hljs-number">1</span></span><span>)
    </span><span><span class="hljs-keyword">return</span></span><span> page

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">crumple</span></span><span>(</span><span><span class="hljs-params">page, folds=<span class="hljs-number">5</span></span></span><span>, noise=</span><span><span class="hljs-number">0.2</span></span><span>):
    </span><span><span class="hljs-string">"""Simulate crumpling: random fold projections + overlap + noise."""</span></span><span>
    size = page.shape[</span><span><span class="hljs-number">0</span></span><span>]
    flat = page.flatten()
    crumpled = np.zeros_like(flat, dtype=</span><span><span class="hljs-built_in">float</span></span><span>)

    </span><span><span class="hljs-keyword">for</span></span><span> _ </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(folds):
        perm = np.random.permutation(</span><span><span class="hljs-built_in">len</span></span><span>(flat))
        folded = flat[perm]
        crumpled += folded

    crumpled /= folds
    crumpled += noise * np.random.randn(</span><span><span class="hljs-built_in">len</span></span><span>(flat))
    </span><span><span class="hljs-keyword">return</span></span><span> crumpled

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">reconstruct</span></span><span>(</span><span><span class="hljs-params">crumpled, page_shape, folds=<span class="hljs-number">5</span></span></span><span>, alpha=</span><span><span class="hljs-number">1.0</span></span><span>):
    </span><span><span class="hljs-string">"""Reconstruct with a simple Ridge regressor (linear denoiser)."""</span></span><span>
    n = np.prod(page_shape)
    </span><span><span class="hljs-comment"># identity-like dictionary with folds</span></span><span>
    X = []
    </span><span><span class="hljs-keyword">for</span></span><span> _ </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(folds):
        perm = np.random.permutation(n)
        X.append(np.eye(n)[perm])
    X = np.mean(X, axis=</span><span><span class="hljs-number">0</span></span><span>)

    reg = Ridge(alpha=alpha, fit_intercept=</span><span><span class="hljs-literal">False</span></span><span>)
    reg.fit(X, crumpled)
    recon = reg.coef_.reshape(page_shape)
    </span><span><span class="hljs-keyword">return</span></span><span> np.clip(recon, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">demo_crumple</span></span><span>():
    page = generate_page(</span><span><span class="hljs-number">32</span></span><span>)
    cr = crumple(page, folds=</span><span><span class="hljs-number">10</span></span><span>, noise=</span><span><span class="hljs-number">0.3</span></span><span>)
    recon = reconstruct(cr, page.shape, folds=</span><span><span class="hljs-number">10</span></span><span>, alpha=</span><span><span class="hljs-number">10.0</span></span><span>)

    fig, axs = plt.subplots(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">3</span></span><span>, figsize=(</span><span><span class="hljs-number">9</span></span><span>, </span><span><span class="hljs-number">3</span></span><span>))
    axs[</span><span><span class="hljs-number">0</span></span><span>].imshow(page, cmap=</span><span><span class="hljs-string">"gray"</span></span><span>)
    axs[</span><span><span class="hljs-number">0</span></span><span>].set_title(</span><span><span class="hljs-string">"Original Page"</span></span><span>)
    axs[</span><span><span class="hljs-number">1</span></span><span>].imshow(cr.reshape(page.shape), cmap=</span><span><span class="hljs-string">"gray"</span></span><span>)
    axs[</span><span><span class="hljs-number">1</span></span><span>].set_title(</span><span><span class="hljs-string">"Crumpled (Entangled)"</span></span><span>)
    axs[</span><span><span class="hljs-number">2</span></span><span>].imshow(recon, cmap=</span><span><span class="hljs-string">"gray"</span></span><span>)
    axs[</span><span><span class="hljs-number">2</span></span><span>].set_title(</span><span><span class="hljs-string">"Reconstruction"</span></span><span>)
    </span><span><span class="hljs-keyword">for</span></span><span> ax </span><span><span class="hljs-keyword">in</span></span><span> axs: ax.axis(</span><span><span class="hljs-string">"off"</span></span><span>)
    plt.tight_layout()
    plt.show()

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    demo_crumple()
