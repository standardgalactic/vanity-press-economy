    tax.ticks(axis=</span><span><span class="hljs-string">'lbr'</span></span><span>, linewidth=</span><span><span class="hljs-number">1</span></span><span>, multiple=</span><span><span class="hljs-number">2</span></span><span>, offset=</span><span><span class="hljs-number">0.02</span></span><span>)
    tax.clear_matplotlib_ticks()
    plt.show()

ternary_plot(Phi_index, v_index, S_index, steps=</span><span><span class="hljs-number">12</span></span><span>)
