.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-comment"># deccelerationist_sim.py</span></span><span>
</span><span><span class="hljs-comment"># RSVP-aligned simulation stubs for The Deccelerationist Agenda</span></span><span>

</span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np
</span><span><span class="hljs-keyword">import</span></span><span> matplotlib.pyplot </span><span><span class="hljs-keyword">as</span></span><span> plt
</span><span><span class="hljs-keyword">from</span></span><span> scipy.integrate </span><span><span class="hljs-keyword">import</span></span><span> solve_ivp
</span><span><span class="hljs-keyword">from</span></span><span> scipy.stats </span><span><span class="hljs-keyword">import</span></span><span> entropy

</span><span><span class="hljs-comment"># -------------------------------</span></span><span>
</span><span><span class="hljs-comment"># 1. Cipher KL divergence vs decoding error</span></span><span>
</span><span><span class="hljs-comment"># -------------------------------</span></span><span>

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">simulate_cipher_decoding_error</span></span><span>(</span><span><span class="hljs-params">vocab=<span class="hljs-number">30</span></span></span><span>, num_students=</span><span><span class="hljs-number">10</span></span><span>, samples=</span><span><span class="hljs-number">500</span></span><span>):
    <span class="hljs-string">"""
    Each student has a personal cipher (random permutation of vocab).
    We simulate model trained only on canonical vocab attempting to decode.
    Decoding error lower-bounded by KL divergence between distributions.
    """</span>
    canonical_dist = np.ones(vocab) / vocab  </span><span><span class="hljs-comment"># uniform corpus</span></span><span>
    results = []

    </span><span><span class="hljs-keyword">for</span></span><span> i </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(num_students):
        perm = np.random.permutation(vocab)
        </span><span><span class="hljs-comment"># student's distribution skewed by cipher bias (zipf-like)</span></span><span>
        student_dist = np.random.zipf(</span><span><span class="hljs-number">1.5</span></span><span>, vocab)
        student_dist = student_dist / student_dist.</span><span><span class="hljs-built_in">sum</span></span><span>()

        </span><span><span class="hljs-comment"># apply cipher permutation</span></span><span>
        ciphered_dist = student_dist[perm]

        </span><span><span class="hljs-comment"># KL divergence between canonical and cipher corpora</span></span><span>
        kl = entropy(ciphered_dist, canonical_dist)

        </span><span><span class="hljs-comment"># simulate decoding error ~ proportional to KL with noise</span></span><span>
        error = kl + </span><span><span class="hljs-number">0.1</span></span><span> * np.random.randn()
        results.append((kl, </span><span><span class="hljs-built_in">max</span></span><span>(error, </span><span><span class="hljs-number">0</span></span><span>)))

    </span><span><span class="hljs-keyword">return</span></span><span> np.array(results)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">plot_cipher_results</span></span><span>(</span><span><span class="hljs-params">results</span></span><span>):
    plt.scatter(results[:,</span><span><span class="hljs-number">0</span></span><span>], results[:,</span><span><span class="hljs-number">1</span></span><span>], c=</span><span><span class="hljs-string">'blue'</span></span><span>)
    plt.xlabel(</span><span><span class="hljs-string">"KL divergence between cipher and canonical corpus"</span></span><span>)
    plt.ylabel(</span><span><span class="hljs-string">"Simulated decoding error"</span></span><span>)
    plt.title(</span><span><span class="hljs-string">"Cipher personalization raises decoding error"</span></span><span>)
    plt.grid(</span><span><span class="hljs-literal">True</span></span><span>)
    plt.show()

</span><span><span class="hljs-comment"># -------------------------------</span></span><span>
</span><span><span class="hljs-comment"># 2. Yogurt-based computation (batch culture)</span></span><span>
</span><span><span class="hljs-comment"># -------------------------------</span></span><span>

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">yogurt_growth</span></span><span>(</span><span><span class="hljs-params">t, y, mu_max, Ks, Yxs</span></span><span>):
    <span class="hljs-string">"""
    Basic Monod kinetics in batch culture (no dilution).
    y = [X, S]
    """</span>
    X, S = y
    mu = mu_max * S / (Ks + S)
    dXdt = mu * X
    dSdt = -(</span><span><span class="hljs-number">1</span></span><span>/Yxs) * mu * X
    </span><span><span class="hljs-keyword">return</span></span><span> [dXdt, dSdt]

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">simulate_yogurt</span></span><span>(</span><span><span class="hljs-params">mu_max=<span class="hljs-number">0.8</span></span></span><span>, Ks=</span><span><span class="hljs-number">0.1</span></span><span>, Yxs=</span><span><span class="hljs-number">0.5</span></span><span>, X0=</span><span><span class="hljs-number">0.01</span></span><span>, S0=</span><span><span class="hljs-number">10</span></span><span>, t_end=</span><span><span class="hljs-number">20</span></span><span>):
    sol = solve_ivp(yogurt_growth, [</span><span><span class="hljs-number">0</span></span><span>, t_end], [X0, S0],
                    args=(mu_max, Ks, Yxs), dense_output=</span><span><span class="hljs-literal">True</span></span><span>)
    t = np.linspace(</span><span><span class="hljs-number">0</span></span><span>, t_end, </span><span><span class="hljs-number">200</span></span><span>)
    X, S = sol.sol(t)
    </span><span><span class="hljs-keyword">return</span></span><span> t, X, S

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">plot_yogurt</span></span><span>(</span><span><span class="hljs-params">t, X, S</span></span><span>):
    fig, ax1 = plt.subplots()
    ax1.plot(t, X, </span><span><span class="hljs-string">'g-'</span></span><span>, label=</span><span><span class="hljs-string">'Biomass X'</span></span><span>)
    ax1.set_xlabel(</span><span><span class="hljs-string">'Time'</span></span><span>)
    ax1.set_ylabel(</span><span><span class="hljs-string">'Biomass'</span></span><span>, color=</span><span><span class="hljs-string">'g'</span></span><span>)
    ax2 = ax1.twinx()
    ax2.plot(t, S, </span><span><span class="hljs-string">'b--'</span></span><span>, label=</span><span><span class="hljs-string">'Substrate S'</span></span><span>)
    ax2.set_ylabel(</span><span><span class="hljs-string">'Substrate'</span></span><span>, color=</span><span><span class="hljs-string">'b'</span></span><span>)
    plt.title(</span><span><span class="hljs-string">"Yogurt culture computation dynamics"</span></span><span>)
    fig.tight_layout()
    plt.show()

</span><span><span class="hljs-comment"># -------------------------------</span></span><span>
</span><span><span class="hljs-comment"># Demo run</span></span><span>
</span><span><span class="hljs-comment"># -------------------------------</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    </span><span><span class="hljs-comment"># Cipher KL vs error</span></span><span>
    results = simulate_cipher_decoding_error()
    plot_cipher_results(results)

    </span><span><span class="hljs-comment"># Yogurt growth</span></span><span>
    t, X, S = simulate_yogurt()
    plot_yogurt(t, X, S)
