</span><span><span class="hljs-keyword">import</span></span><span> ternary

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">ternary_plot</span></span><span>(</span><span><span class="hljs-params">Phi, v, S, steps=<span class="hljs-number">10</span></span></span><span>):
    <span class="hljs-string">"""
    Plot Decceleration Index across ternary weight space (Φ, v, S).
    """</span>
    scale = steps
    data = {}
    </span><span><span class="hljs-keyword">for</span></span><span> a </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>):
        </span><span><span class="hljs-keyword">for</span></span><span> b </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>-a):
            c = scale - a - b
            weights = (a/scale, b/scale, c/scale)
            idx = decceleration_index(Phi, v, S, weights)
            data[(a, b, c)] = idx

    </span><span><span class="hljs-comment"># Normalize index values for colormap</span></span><span>
    vals = np.array(</span><span><span class="hljs-built_in">list</span></span><span>(data.values()))
    min_val, max_val = vals.</span><span><span class="hljs-built_in">min</span></span><span>(), vals.</span><span><span class="hljs-built_in">max</span></span><span>()

    figure, tax = ternary.figure(scale=scale)
    tax.set_title(</span><span><span class="hljs-string">"Decceleration Index Sensitivity (Φ, v, S)"</span></span><span>, fontsize=</span><span><span class="hljs-number">14</span></span><span>)

    </span><span><span class="hljs-comment"># Heatmap points</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> (a,b,c), val </span><span><span class="hljs-keyword">in</span></span><span> data.items():
        color_val = (val-min_val)/(max_val-min_val+</span><span><span class="hljs-number">1e-9</span></span><span>)
        tax.scatter([(a,b,c)], marker=</span><span><span class="hljs-string">'o'</span></span><span>, color=plt.cm.plasma(color_val), s=</span><span><span class="hljs-number">40</span></span><span>)

    </span><span><span class="hljs-comment"># Labels</span></span><span>
    tax.left_axis_label(</span><span><span class="hljs-string">r"$\mathbf v$ (Flow)"</span></span><span>, fontsize=</span><span><span class="hljs-number">12</span></span><span>)
