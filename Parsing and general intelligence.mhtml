plt.plot(t, idx_div, </span><span><span class="hljs-string">'m-'</span></span><span>, linewidth=</span><span><span class="hljs-number">2</span></span><span>, label=</span><span><span class="hljs-string">"Deccelerationist Diversification"</span></span><span>)
plt.fill_between(t, idx_base, idx_div, color=</span><span><span class="hljs-string">'purple'</span></span><span>, alpha=</span><span><span class="hljs-number">0.2</span></span><span>,
                 label=</span><span><span class="hljs-string">"Slowdown Gap (Resilience Gain)"</span></span><span>)
