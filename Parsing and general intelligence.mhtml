</span><span><span class="hljs-keyword">import</span></span><span> ternary

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">ternary_compare</span></span><span>(</span><span><span class="hljs-params">Phi_base, v_base, S_base, Phi_div, v_div, S_div, steps=<span class="hljs-number">12</span></span></span><span>):
    scale = steps
    data_base, data_div = {}, {}

    </span><span><span class="hljs-keyword">for</span></span><span> a </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>):
        </span><span><span class="hljs-keyword">for</span></span><span> b </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>-a):
            c = scale - a - b
            weights = (a/scale, b/scale, c/scale)
            data_base[(a,b,c)] = decceleration_index(Phi_base, v_base, S_base, weights)
            data_div[(a,b,c)]  = decceleration_index(Phi_div,  v_div,  S_div,  weights)

    </span><span><span class="hljs-comment"># Normalize both</span></span><span>
    vals_base = np.array(</span><span><span class="hljs-built_in">list</span></span><span>(data_base.values()))
    vals_div  = np.array(</span><span><span class="hljs-built_in">list</span></span><span>(data_div.values()))
    max_val = </span><span><span class="hljs-built_in">max</span></span><span>(vals_base.</span><span><span class="hljs-built_in">max</span></span><span>(), vals_div.</span><span><span class="hljs-built_in">max</span></span><span>())
    min_val = </span><span><span class="hljs-built_in">min</span></span><span>(vals_base.</span><span><span class="hljs-built_in">min</span></span><span>(), vals_div.</span><span><span class="hljs-built_in">min</span></span><span>())

    fig, (ax1, ax2) = plt.subplots(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>, figsize=(</span><span><span class="hljs-number">12</span></span><span>,</span><span><span class="hljs-number">6</span></span><span>))

    </span><span><span class="hljs-comment"># Left: Baseline</span></span><span>
    tax1 = ternary.TernaryAxesSubplot(ax=ax1, scale=scale)
    </span><span><span class="hljs-keyword">for</span></span><span> (a,b,c), val </span><span><span class="hljs-keyword">in</span></span><span> data_base.items():
