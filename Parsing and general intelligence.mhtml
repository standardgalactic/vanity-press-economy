</span><span><span class="hljs-attr">on:</span></span><span>
  </span><span><span class="hljs-attr">push:</span></span><span>
    </span><span><span class="hljs-attr">branches:</span></span><span> [</span><span><span class="hljs-string">"main"</span></span><span>]
  </span><span><span class="hljs-attr">pull_request:</span></span><span>

</span><span><span class="hljs-attr">jobs:</span></span><span>
  </span><span><span class="hljs-attr">build:</span></span><span>
    </span><span><span class="hljs-attr">runs-on:</span></span><span> </span><span><span class="hljs-string">ubuntu-latest</span></span><span>

    </span><span><span class="hljs-attr">steps:</span></span><span>
      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Checkout</span></span><span> </span><span><span class="hljs-string">repo</span></span><span>
        </span><span><span class="hljs-attr">uses:</span></span><span> </span><span><span class="hljs-string">actions/checkout@v4</span></span><span>

      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Set</span></span><span> </span><span><span class="hljs-string">up</span></span><span> </span><span><span class="hljs-string">Python</span></span><span>
        </span><span><span class="hljs-attr">uses:</span></span><span> </span><span><span class="hljs-string">actions/setup-python@v5</span></span><span>
        </span><span><span class="hljs-attr">with:</span></span><span>
          </span><span><span class="hljs-attr">python-version:</span></span><span> </span><span><span class="hljs-string">"3.10"</span></span><span>

      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Install</span></span><span> </span><span><span class="hljs-string">Python</span></span><span> </span><span><span class="hljs-string">dependencies</span></span><span>
        </span><span><span class="hljs-attr">run:</span></span><span> <span class="hljs-string">|
          python -m pip install --upgrade pip
          pip install -r requirements.txt
          pip install nbconvert jupyter
</span>
      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Run</span></span><span> </span><span><span class="hljs-string">notebook</span></span><span> </span><span><span class="hljs-string">and</span></span><span> </span><span><span class="hljs-string">export</span></span><span> </span><span><span class="hljs-string">figures</span></span><span>
        </span><span><span class="hljs-attr">run:</span></span><span> </span><span><span class="hljs-string">make</span></span><span> </span><span><span class="hljs-string">export</span></span><span>

      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Set</span></span><span> </span><span><span class="hljs-string">up</span></span><span> </span><span><span class="hljs-string">LaTeX</span></span><span>
        </span><span><span class="hljs-attr">uses:</span></span><span> </span><span><span class="hljs-string">xu-cheng/latex-action@v3</span></span><span>
        </span><span><span class="hljs-attr">with:</span></span><span>
          </span><span><span class="hljs-attr">root_file:</span></span><span> </span><span><span class="hljs-string">paper/deccelerationist_agenda.tex</span></span><span>
          </span><span><span class="hljs-attr">working_directory:</span></span><span> </span><span><span class="hljs-string">paper</span></span><span>
          </span><span><span class="hljs-attr">args:</span></span><span> </span><span><span class="hljs-string">"-interaction=nonstopmode"</span></span><span>

      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Upload</span></span><span> </span><span><span class="hljs-string">PDF</span></span><span> </span><span><span class="hljs-string">artifact</span></span><span>
        </span><span><span class="hljs-attr">uses:</span></span><span> </span><span><span class="hljs-string">actions/upload-artifact@v4</span></span><span>
        </span><span><span class="hljs-attr">with:</span></span><span>
          </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">paper-pdf</span></span><span>
          </span><span><span class="hljs-attr">path:</span></span><span> </span><span><span class="hljs-string">paper/deccelerationist_agenda.pdf</span></span><span>

      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-attr">name:</span></span><span> </span><span><span class="hljs-string">Prepare</span></span><span> </span><span><span class="hljs-string">GitHub</span></span><span> </span><span><span class="hljs-string">Pages</span></span><span>
        </span><span><span class="hljs-attr">run:</span></span><span> <span class="hljs-string">|
