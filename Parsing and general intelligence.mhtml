        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.vocab = vocab
        self.max_len = max_len

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">batch</span></span><span>(</span><span><span class="hljs-params">self, B=<span class="hljs-number">32</span></span></span><span>) -&gt; </span><span><span class="hljs-type">Tuple</span></span><span>[torch.Tensor, torch.Tensor]:
        T = random.randint(self.max_len//</span><span><span class="hljs-number">2</span></span><span>, self.max_len)
        x = torch.randint(</span><span><span class="hljs-number">0</span></span><span>, self.vocab, (B, T), device=DEVICE)
        y = x.clone()
        </span><span><span class="hljs-comment"># small corruption to avoid triviality</span></span><span>
        noise_mask = (torch.rand_like(y.</span><span><span class="hljs-built_in">float</span></span><span>()) &lt; </span><span><span class="hljs-number">0.05</span></span><span>)
        y[noise_mask] = torch.randint(</span><span><span class="hljs-number">0</span></span><span>, self.vocab, y.shape, device=DEVICE)[noise_mask]
        </span><span><span class="hljs-keyword">return</span></span><span> x, y

</span><span><span class="hljs-comment"># -----------------------------</span></span><span>
</span><span><span class="hljs-comment"># Parsers and components</span></span><span>
</span><span><span class="hljs-comment"># -----------------------------</span></span><span>

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">Encoder</span></span><span>(nn.Module):
    </span><span><span class="hljs-string">"""ηθ: raw tokens -&gt; latent representation P (parser output)."""</span></span><span>
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, vocab=<span class="hljs-number">256</span></span></span><span>, d_model=</span><span><span class="hljs-number">512</span></span><span>, n_layers=</span><span><span class="hljs-number">4</span></span><span>, n_heads=</span><span><span class="hljs-number">8</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.emb = nn.Embedding(vocab, d_model)
        encoder_layer = nn.TransformerEncoderLayer(d_model, n_heads, dim_feedforward=</span><span><span class="hljs-number">4</span></span><span>*d_model, batch_first=</span><span><span class="hljs-literal">True</span></span><span>)
        self.tr = nn.TransformerEncoder(encoder_layer, num_layers=n_layers)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, tokens</span></span><span>):  </span><span><span class="hljs-comment"># tokens: [B, T]</span></span><span>
        z = self.emb(tokens)
        p = self.tr(z)  </span><span><span class="hljs-comment"># [B, T, d_model]</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> p

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">Merger</span></span><span>(nn.Module):
    </span><span><span class="hljs-string">"""μθ: flatten G(G(x)) -&gt; G(x)."""</span></span><span>
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, d_model=<span class="hljs-number">512</span></span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.lin = nn.Sequential(
            nn.Linear(</span><span><span class="hljs-number">2</span></span><span>*d_model, d_model),
            nn.GELU(),
            nn.Linear(d_model, d_model)
        )

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, p1, p2</span></span><span>):
        </span><span><span class="hljs-comment"># p1,p2: [B, T, D]; concatenate features, reduce</span></span><span>
        T = </span><span><span class="hljs-built_in">min</span></span><span>(p1.shape[</span><span><span class="hljs-number">1</span></span><span>], p2.shape[</span><span><span class="hljs-number">1</span></span><span>])
        h = torch.cat([p1[:, :T, :], p2[:, :T, :]], dim=-</span><span><span class="hljs-number">1</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span> self.lin(h)

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">Realizer</span></span><span>(nn.Module):
    </span><span><span class="hljs-string">"""αθ: latent parse -&gt; tokens/logits (executor / EM-algebra)."""</span></span><span>
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, d_model=<span class="hljs-number">512</span></span></span><span>, vocab=</span><span><span class="hljs-number">256</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.proj = nn.Linear(d_model, vocab)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, p</span></span><span>):  </span><span><span class="hljs-comment"># [B, T, D] -&gt; [B, T, V]</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> self.proj(p)

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">CapacityGate</span></span><span>(nn.Module):
    </span><span><span class="hljs-string">"""Φθ: capacity allocation; outputs gates in [0,1] with global budget B."""</span></span><span>
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, d_model=<span class="hljs-number">512</span></span></span><span>, budget_ratio=</span><span><span class="hljs-number">0.5</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.score = nn.Sequential(
            nn.Linear(d_model, d_model//</span><span><span class="hljs-number">2</span></span><span>),
            nn.GELU(),
            nn.Linear(d_model//</span><span><span class="hljs-number">2</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)
        )
        self.budget_ratio = budget_ratio

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, p</span></span><span>):
        </span><span><span class="hljs-comment"># p: [B, T, D]</span></span><span>
        raw = self.score(p).squeeze(-</span><span><span class="hljs-number">1</span></span><span>)  </span><span><span class="hljs-comment"># [B, T]</span></span><span>
        g = torch.sigmoid(raw)
        </span><span><span class="hljs-comment"># project to global budget per-batch</span></span><span>
        B = </span><span><span class="hljs-built_in">int</span></span><span>(self.budget_ratio * g.shape[</span><span><span class="hljs-number">1</span></span><span>])
        </span><span><span class="hljs-comment"># keep top-B tokens (softly): topk -&gt; temperature-softmax</span></span><span>
        topk_vals, _ = torch.topk(g, k=</span><span><span class="hljs-built_in">max</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>, B), dim=</span><span><span class="hljs-number">1</span></span><span>)
        thresh = topk_vals[:, -</span><span><span class="hljs-number">1</span></span><span>:].detach()
        g_soft = torch.sigmoid(</span><span><span class="hljs-number">10.0</span></span><span> * (g - thresh))  </span><span><span class="hljs-comment"># sharpen near threshold</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> g_soft  </span><span><span class="hljs-comment"># [B, T] in (0,1)</span></span><span>

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">Router</span></span><span>(nn.Module):
