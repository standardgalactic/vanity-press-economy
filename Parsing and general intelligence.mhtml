</span><span><span class="hljs-keyword">import</span></span><span> math, random
</span><span><span class="hljs-keyword">from</span></span><span> dataclasses </span><span><span class="hljs-keyword">import</span></span><span> dataclass
</span><span><span class="hljs-keyword">from</span></span><span> typing </span><span><span class="hljs-keyword">import</span></span><span> </span><span><span class="hljs-type">Dict</span></span><span>, </span><span><span class="hljs-type">Tuple</span></span><span>

</span><span><span class="hljs-keyword">import</span></span><span> torch
</span><span><span class="hljs-keyword">import</span></span><span> torch.nn </span><span><span class="hljs-keyword">as</span></span><span> nn
</span><span><span class="hljs-keyword">import</span></span><span> torch.nn.functional </span><span><span class="hljs-keyword">as</span></span><span> F

DEVICE = </span><span><span class="hljs-string">"cuda"</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> torch.cuda.is_available() </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">"cpu"</span></span><span>
DTYPE = torch.float32
