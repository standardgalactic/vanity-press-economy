    _fig_counter[</span><span><span class="hljs-string">"count"</span></span><span>] += </span><span><span class="hljs-number">1</span></span><span>
    fname = name </span><span><span class="hljs-keyword">if</span></span><span> name </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">f"figure_<span class="hljs-subst">{_fig_counter[<span class="hljs-string">'count'</span></span></span></span><span>]:02d}.png"
    path = os.path.join(FIGURE_DIR, fname)
    fig.savefig(path, dpi=dpi, bbox_inches=</span><span><span class="hljs-string">"tight"</span></span><span>)
    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Saved figure -&gt; <span class="hljs-subst">{path}</span></span></span><span>")
