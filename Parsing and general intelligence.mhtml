</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">compute_metrics</span></span><span>(</span><span><span class="hljs-params">cipher_results, X, S, page, recon</span></span><span>):
    <span class="hljs-string">"""
    Compute proxy metrics for Φ (capacity), v (flow), and S (entropy).
    """</span>
    </span><span><span class="hljs-comment"># From cipher sim: KL divergence ~ Φ inflation + S diversity</span></span><span>
    avg_kl = np.mean(cipher_results[:,</span><span><span class="hljs-number">0</span></span><span>])
    
    </span><span><span class="hljs-comment"># From yogurt sim: normalized growth area under curve ~ added Φ_bio</span></span><span>
    bio_capacity = np.trapz(X, dx=(t[</span><span><span class="hljs-number">1</span></span><span>]-t[</span><span><span class="hljs-number">0</span></span><span>])) / </span><span><span class="hljs-built_in">max</span></span><span>(X)
    
    </span><span><span class="hljs-comment"># From crumple sim: reconstruction quality vs entropy</span></span><span>
    mse = np.mean((page - recon)**</span><span><span class="hljs-number">2</span></span><span>)
    entropy_increase = np.log(</span><span><span class="hljs-number">1</span></span><span> + mse * </span><span><span class="hljs-number">100</span></span><span>)  </span><span><span class="hljs-comment"># scaled entropy proxy</span></span><span>
    
    </span><span><span class="hljs-comment"># Aggregate indices</span></span><span>
    Phi_index = avg_kl + bio_capacity
    v_index   = np.std(cipher_results[:,</span><span><span class="hljs-number">1</span></span><span>])   </span><span><span class="hljs-comment"># variability in decoding error as flow diversity</span></span><span>
    S_index   = entropy_increase
    
    </span><span><span class="hljs-keyword">return</span></span><span> Phi_index, v_index, S_index

Phi_index, v_index, S_index = compute_metrics(results, X, S, page, recon)

</span><span><span class="hljs-comment"># Normalize for visualization</span></span><span>
indices = np.array([Phi_index, v_index, S_index])
labels = [</span><span><span class="hljs-string">r"$\Phi$ (Capacity)"</span></span><span>, </span><span><span class="hljs-string">r"$\mathbf{v}$ (Flow)"</span></span><span>, </span><span><span class="hljs-string">r"$S$ (Entropy)"</span></span><span>]
indices_norm = indices / indices.</span><span><span class="hljs-built_in">max</span></span><span>()

plt.figure(figsize=(</span><span><span class="hljs-number">6</span></span><span>,</span><span><span class="hljs-number">4</span></span><span>))
bars = plt.bar(labels, indices_norm, color=[</span><span><span class="hljs-string">'red'</span></span><span>,</span><span><span class="hljs-string">'green'</span></span><span>,</span><span><span class="hljs-string">'blue'</span></span><span>])
plt.ylim(</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">1.2</span></span><span>)
plt.title(</span><span><span class="hljs-string">"RSVP Slowdown Strength Indices"</span></span><span>)
plt.ylabel(</span><span><span class="hljs-string">"Normalized Index Value"</span></span><span>)
</span><span><span class="hljs-keyword">for</span></span><span> bar, val </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(bars, indices):
    plt.text(bar.get_x() + bar.get_width()/</span><span><span class="hljs-number">2</span></span><span>, bar.get_height()+</span><span><span class="hljs-number">0.02</span></span><span>,
             </span><span><span class="hljs-string">f"<span class="hljs-subst">{val:<span class="hljs-number">.2</span></span></span></span><span>f}", ha=</span><span><span class="hljs-string">'center'</span></span><span>, va=</span><span><span class="hljs-string">'bottom'</span></span><span>)
plt.show()
</span></span></code></div></div></pre>
