    tax1.left_axis_label(</span><span><span class="hljs-string">r"$\mathbf v$ (Flow)"</span></span><span>)
    tax1.right_axis_label(</span><span><span class="hljs-string">r"$S$ (Entropy)"</span></span><span>)
    tax1.bottom_axis_label(</span><span><span class="hljs-string">r"$\Phi$ (Capacity)"</span></span><span>)
    tax1.set_title(</span><span><span class="hljs-string">"Baseline Monoculture"</span></span><span>)
    tax1.clear_matplotlib_ticks()

    </span><span><span class="hljs-comment"># Right: Diversified</span></span><span>
    tax2 = ternary.TernaryAxesSubplot(ax=ax2, scale=scale)
    </span><span><span class="hljs-keyword">for</span></span><span> (a,b,c), val </span><span><span class="hljs-keyword">in</span></span><span> data_div.items():
        color_val = (val-min_val)/(max_val-min_val+</span><span><span class="hljs-number">1e-9</span></span><span>)
        tax2.scatter([(a,b,c)], marker=</span><span><span class="hljs-string">'o'</span></span><span>, color=plt.cm.plasma(color_val), s=</span><span><span class="hljs-number">35</span></span><span>)
    tax2.boundary()
    tax2.gridlines(multiple=</span><span><span class="hljs-number">3</span></span><span>, color=</span><span><span class="hljs-string">"gray"</span></span><span>, linewidth=</span><span><span class="hljs-number">0.5</span></span><span>)
    tax2.left_axis_label(</span><span><span class="hljs-string">r"$\mathbf v$ (Flow)"</span></span><span>)
    tax2.right_axis_label(</span><span><span class="hljs-string">r"$S$ (Entropy)"</span></span><span>)
    tax2.bottom_axis_label(</span><span><span class="hljs-string">r"$\Phi$ (Capacity)"</span></span><span>)
    tax2.set_title(</span><span><span class="hljs-string">"Deccelerationist Diversification"</span></span><span>)
    tax2.clear_matplotlib_ticks()

