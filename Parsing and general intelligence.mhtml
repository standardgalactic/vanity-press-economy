</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">cumulative_gap</span></span><span>(</span><span><span class="hljs-params">Phi_base, v_base, S_base, Phi_div, v_div, S_div, years=<span class="hljs-number">30</span></span></span><span>, weights=(</span><span><span class="hljs-params"><span class="hljs-number">0.4</span></span></span><span>,</span><span><span class="hljs-number">0.3</span></span><span>,</span><span><span class="hljs-number">0.3</span></span><span>)):
    t = np.arange(</span><span><span class="hljs-number">0</span></span><span>, years+</span><span><span class="hljs-number">1</span></span><span>)

    </span><span><span class="hljs-comment"># Baseline trajectory</span></span><span>
    Phi_b = Phi_base * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.005</span></span><span>*t)
    v_b   = v_base   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.002</span></span><span>*t)
    S_b   = S_base   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.001</span></span><span>*t)
    idx_base = decceleration_index(Phi_b, v_b, S_b, weights)

    </span><span><span class="hljs-comment"># Diversified trajectory</span></span><span>
    Phi_d = Phi_div * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.03</span></span><span>*t)
    v_d   = v_div   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.02</span></span><span>*t)
    S_d   = S_div   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.015</span></span><span>*t)
    idx_div = decceleration_index(Phi_d, v_d, S_d, weights)

    </span><span><span class="hljs-comment"># Slowdown gap = diversified – baseline</span></span><span>
    gap = idx_div - idx_base
    cumulative_gap = np.cumsum(gap)

    </span><span><span class="hljs-keyword">return</span></span><span> t, idx_base, idx_div, gap, cumulative_gap

