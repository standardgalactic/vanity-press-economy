</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">ternary_time_evolution</span></span><span>(</span><span><span class="hljs-params">Phi_base, v_base, S_base, Phi_div, v_div, S_div, years=<span class="hljs-number">10</span></span></span><span>, steps=</span><span><span class="hljs-number">10</span></span><span>):
    <span class="hljs-string">"""
    Generate time-lapse ternary plots showing baseline vs diversified Decceleration Index evolution.
    """</span>
    scale = steps
    fig, axes = plt.subplots(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>, figsize=(</span><span><span class="hljs-number">12</span></span><span>,</span><span><span class="hljs-number">6</span></span><span>))
    ax_base, ax_div = axes

    tax_base = ternary.TernaryAxesSubplot(ax=ax_base, scale=scale)
    tax_div  = ternary.TernaryAxesSubplot(ax=ax_div, scale=scale)
    
    tax_base.boundary(); tax_base.clear_matplotlib_ticks()
    tax_div.boundary();  tax_div.clear_matplotlib_ticks()

    tax_base.left_axis_label(</span><span><span class="hljs-string">r"$\mathbf v$"</span></span><span>)
    tax_base.right_axis_label(</span><span><span class="hljs-string">r"$S$"</span></span><span>)
    tax_base.bottom_axis_label(</span><span><span class="hljs-string">r"$\Phi$"</span></span><span>)
    tax_base.set_title(</span><span><span class="hljs-string">"Baseline Monoculture"</span></span><span>)

    tax_div.left_axis_label(</span><span><span class="hljs-string">r"$\mathbf v$"</span></span><span>)
    tax_div.right_axis_label(</span><span><span class="hljs-string">r"$S$"</span></span><span>)
    tax_div.bottom_axis_label(</span><span><span class="hljs-string">r"$\Phi$"</span></span><span>)
    tax_div.set_title(</span><span><span class="hljs-string">"Diversified Regime"</span></span><span>)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">frame_update</span></span><span>(</span><span><span class="hljs-params">year</span></span><span>):
        </span><span><span class="hljs-keyword">for</span></span><span> tax </span><span><span class="hljs-keyword">in</span></span><span> (tax_base, tax_div): tax.clear_matplotlib_ticks()

        </span><span><span class="hljs-comment"># Baseline evolution (almost flat)</span></span><span>
        Phi_b = Phi_base * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.005</span></span><span>*year)
        v_b   = v_base   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.002</span></span><span>*year)
        S_b   = S_base   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.001</span></span><span>*year)

        </span><span><span class="hljs-comment"># Diversified evolution (compounding)</span></span><span>
        Phi_d = Phi_div * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.03</span></span><span>*year)
        v_d   = v_div   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.02</span></span><span>*year)
        S_d   = S_div   * (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.015</span></span><span>*year)

        </span><span><span class="hljs-comment"># Compute index values</span></span><span>
        data_b, data_d = {}, {}
        </span><span><span class="hljs-keyword">for</span></span><span> a </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>):
            </span><span><span class="hljs-keyword">for</span></span><span> b </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(scale+</span><span><span class="hljs-number">1</span></span><span>-a):
                c = scale - a - b
                w = (a/scale, b/scale, c/scale)
                data_b[(a,b,c)] = decceleration_index(Phi_b, v_b, S_b, w)
                data_d[(a,b,c)] = decceleration_index(Phi_d, v_d, S_d, w)

        </span><span><span class="hljs-comment"># Normalize</span></span><span>
        vals = </span><span><span class="hljs-built_in">list</span></span><span>(data_b.values()) + </span><span><span class="hljs-built_in">list</span></span><span>(data_d.values())
        min_val, max_val = </span><span><span class="hljs-built_in">min</span></span><span>(vals), </span><span><span class="hljs-built_in">max</span></span><span>(vals)

        tax_base.clear_matplotlib_ticks(); tax_base.boundary()
        </span><span><span class="hljs-keyword">for</span></span><span> (a,b,c), val </span><span><span class="hljs-keyword">in</span></span><span> data_b.items():
            col = plt.cm.plasma((val-min_val)/(max_val-min_val+</span><span><span class="hljs-number">1e-9</span></span><span>))
            tax_base.scatter([(a,b,c)], color=col, s=</span><span><span class="hljs-number">35</span></span><span>)

        tax_div.clear_matplotlib_ticks(); tax_div.boundary()
        </span><span><span class="hljs-keyword">for</span></span><span> (a,b,c), val </span><span><span class="hljs-keyword">in</span></span><span> data_d.items():
            col = plt.cm.plasma((val-min_val)/(max_val-min_val+</span><span><span class="hljs-number">1e-9</span></span><span>))
            tax_div.scatter([(a,b,c)], color=col, s=</span><span><span class="hljs-number">35</span></span><span>)

        fig.suptitle(</span><span><span class="hljs-string">f"Ternary Evolution (Year <span class="hljs-subst">{year}</span></span></span><span>)", fontsize=</span><span><span class="hljs-number">14</span></span><span>)

    ani = animation.FuncAnimation(fig, frame_update, frames=years+</span><span><span class="hljs-number">1</span></span><span>, interval=</span><span><span class="hljs-number">800</span></span><span>, repeat=</span><span><span class="hljs-literal">True</span></span><span>)
    plt.close(fig)
    </span><span><span class="hljs-keyword">return</span></span><span> ani

</span><span><span class="hljs-comment"># Run and display animation (in Jupyter this will render inline)</span></span><span>
ani = ternary_time_evolution(Phi_base=</span><span><span class="hljs-number">0.1</span></span><span>, v_base=</span><span><span class="hljs-number">0.05</span></span><span>, S_base=</span><span><span class="hljs-number">0.02</span></span><span>,
                             Phi_div=Phi_index, v_div=v_index, S_div=S_index,
                             years=</span><span><span class="hljs-number">15</span></span><span>, steps=</span><span><span class="hljs-number">10</span></span><span>)

</span><span><span class="hljs-keyword">from</span></span><span> IPython.display </span><span><span class="hljs-keyword">import</span></span><span> HTML
HTML(ani.to_jshtml())
