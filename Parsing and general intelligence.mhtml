    </span><span><span class="hljs-string">"    for _ in range(folds):\n"</span></span><span><span class="hljs-punctuation">,</span></span><span>
    </span><span><span class="hljs-string">"        perm=np.random.permutation(n)\n"</span></span><span><span class="hljs-punctuation">,</span></span><span>
    </span><span><span class="hljs-string">"        X.append(np.eye(n)[perm])\n"</span></span><span><span class="hljs-punctuation">,</span></span><span>
