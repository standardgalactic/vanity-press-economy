        error = kl + </span><span><span class="hljs-number">0.1</span></span><span> * np.random.randn()
        results.append((kl, </span><span><span class="hljs-built_in">max</span></span><span>(error, </span><span><span class="hljs-number">0</span></span><span>)))
    </span><span><span class="hljs-keyword">return</span></span><span> np.array(results)

