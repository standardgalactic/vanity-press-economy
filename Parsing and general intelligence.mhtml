    </span><span><span class="hljs-keyword">for</span></span><span> a </span><span><span class="hljs-keyword">in</span></span><span> np.linspace(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, steps):
