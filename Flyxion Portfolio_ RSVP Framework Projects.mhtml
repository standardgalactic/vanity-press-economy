<span class="line"><span style="color: rgb(106, 153, 85);"># Poisson solver using FFT (periodic BC)</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">def</span><span style="color: rgb(220, 220, 170);"> poisson_solve</span><span style="color: rgb(230, 230, 230);">(</span><span style="color: rgb(156, 220, 254);">rhs</span><span style="color: rgb(230, 230, 230);">):</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    kx </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> np.fft.fftfreq(nx, </span><span style="color: rgb(156, 220, 254);">d</span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);">dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    ky </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> np.fft.fftfreq(ny, </span><span style="color: rgb(156, 220, 254);">d</span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);">dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    KX, KY </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> np.meshgrid(kx, ky)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    K2 </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> KX</span><span style="color: rgb(212, 212, 212);">**</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(212, 212, 212);"> +</span><span style="color: rgb(230, 230, 230);"> KY</span><span style="color: rgb(212, 212, 212);">**</span><span style="color: rgb(181, 206, 168);">2</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    K2[K2 </span><span style="color: rgb(212, 212, 212);">==</span><span style="color: rgb(181, 206, 168);"> 0</span><span style="color: rgb(230, 230, 230);">] </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(181, 206, 168);"> 1e-10</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    p_hat </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> fft2(rhs) </span><span style="color: rgb(212, 212, 212);">/</span><span style="color: rgb(230, 230, 230);"> (</span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);">K2)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    p </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> np.real(ifft2(p_hat))</span></span>
<span class="line"><span style="color: rgb(197, 134, 192);">    return</span><span style="color: rgb(230, 230, 230);"> p</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(106, 153, 85);"># Compute div v</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">def</span><span style="color: rgb(220, 220, 170);"> div_v</span><span style="color: rgb(230, 230, 230);">(</span><span style="color: rgb(156, 220, 254);">vx</span><span style="color: rgb(230, 230, 230);">, </span><span style="color: rgb(156, 220, 254);">vy</span><span style="color: rgb(230, 230, 230);">):</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(vx, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(vy, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(197, 134, 192);">    return</span><span style="color: rgb(230, 230, 230);"> div_x </span><span style="color: rgb(212, 212, 212);">+</span><span style="color: rgb(230, 230, 230);"> div_y</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(106, 153, 85);"># Project v to div v = J (here J=0)</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">def</span><span style="color: rgb(220, 220, 170);"> project_v</span><span style="color: rgb(230, 230, 230);">(</span><span style="color: rgb(156, 220, 254);">vx</span><span style="color: rgb(230, 230, 230);">, </span><span style="color: rgb(156, 220, 254);">vy</span><span style="color: rgb(230, 230, 230);">, </span><span style="color: rgb(156, 220, 254);">J</span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(181, 206, 168);">0.0</span><span style="color: rgb(230, 230, 230);">):</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_v(vx, vy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    rhs </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> J</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    p </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> poisson_solve(rhs)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    grad_p_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(p, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    grad_p_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(p, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vx </span><span style="color: rgb(212, 212, 212);">-=</span><span style="color: rgb(230, 230, 230);"> grad_p_x</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vy </span><span style="color: rgb(212, 212, 212);">-=</span><span style="color: rgb(230, 230, 230);"> grad_p_y</span></span>
<span class="line"><span style="color: rgb(197, 134, 192);">    return</span><span style="color: rgb(230, 230, 230);"> vx, vy</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(106, 153, 85);"># FV operator</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">def</span><span style="color: rgb(220, 220, 170);"> fv_update</span><span style="color: rgb(230, 230, 230);">(</span><span style="color: rgb(156, 220, 254);">U</span><span style="color: rgb(230, 230, 230);">):</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    Phi, Pi, S, vx, vy </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> U[</span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">], U[</span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">], U[</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(230, 230, 230);">], U[</span><span style="color: rgb(181, 206, 168);">3</span><span style="color: rgb(230, 230, 230);">], U[</span><span style="color: rgb(181, 206, 168);">4</span><span style="color: rgb(230, 230, 230);">]</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # Pi fluxes x</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    Phi_Lx, Phi_Rx </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(Phi, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vx_Lx, vx_Rx </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(vx, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_diff_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);"> (Phi_Rx </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> Phi_Lx) </span><span style="color: rgb(212, 212, 212);">/</span><span style="color: rgb(230, 230, 230);"> dx</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_adv_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vx_Lx, vx_Rx, Phi_Lx, Phi_Rx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> F_Pi_diff_x </span><span style="color: rgb(212, 212, 212);">+</span><span style="color: rgb(230, 230, 230);"> F_Pi_adv_x</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # Pi fluxes y</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    Phi_Ly, Phi_Ry </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(Phi, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vy_Ly, vy_Ry </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(vy, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_diff_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);"> (Phi_Ry </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> Phi_Ly) </span><span style="color: rgb(212, 212, 212);">/</span><span style="color: rgb(230, 230, 230);"> dy</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_adv_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vy_Ly, vy_Ry, Phi_Ly, Phi_Ry)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_Pi_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> F_Pi_diff_y </span><span style="color: rgb(212, 212, 212);">+</span><span style="color: rgb(230, 230, 230);"> F_Pi_adv_y</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # div F_Pi</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_Pi_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_Pi_x, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_Pi_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_Pi_y, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # dPi/dt</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    source_Pi </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);">kappa </span><span style="color: rgb(212, 212, 212);">*</span><span style="color: rgb(230, 230, 230);"> S</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    dPi_dt </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);"> div_F_Pi_x </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> div_F_Pi_y </span><span style="color: rgb(212, 212, 212);">+</span><span style="color: rgb(230, 230, 230);"> source_Pi</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # S fluxes x</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    S_Lx, S_Rx </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(S, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_S_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vx_Lx, vx_Rx, S_Lx, S_Rx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # S fluxes y</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    S_Ly, S_Ry </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(S, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_S_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vy_Ly, vy_Ry, S_Ly, S_Ry)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # div F_S</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_S_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_S_x, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_S_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_S_y, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # source_S</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    grad_Phi_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(Phi, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    grad_Phi_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> central_grad(Phi, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    grad_Phi_sq </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> grad_Phi_x</span><span style="color: rgb(212, 212, 212);">**</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(212, 212, 212);"> +</span><span style="color: rgb(230, 230, 230);"> grad_Phi_y</span><span style="color: rgb(212, 212, 212);">**</span><span style="color: rgb(181, 206, 168);">2</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    source_S </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);">lambda_ </span><span style="color: rgb(212, 212, 212);">*</span><span style="color: rgb(230, 230, 230);"> grad_Phi_sq</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    dS_dt </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);"> div_F_S_x </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> div_F_S_y </span><span style="color: rgb(212, 212, 212);">+</span><span style="color: rgb(230, 230, 230);"> source_S</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # dPhi/dt = Pi</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    dPhi_dt </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> Pi</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # v_x fluxes</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vx_Ly, vx_Ry </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(vx, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_vx_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vx_Lx, vx_Rx, vx_Lx, vx_Rx)  </span><span style="color: rgb(106, 153, 85);"># for partial_x (vx^2)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_vx_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vy_Ly, vy_Ry, vx_Ly, vx_Ry)  </span><span style="color: rgb(106, 153, 85);"># for partial_y (vx vy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_vx_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_vx_x, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_vx_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_vx_y, </span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(230, 230, 230);">, dy)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    dvx_dt </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(212, 212, 212);"> -</span><span style="color: rgb(230, 230, 230);"> div_F_vx_x </span><span style="color: rgb(212, 212, 212);">-</span><span style="color: rgb(230, 230, 230);"> div_F_vx_y</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(106, 153, 85);">    # v_y fluxes</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    vy_Lx, vy_Rx </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> reconstruct(vy, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_vy_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vx_Lx, vx_Rx, vy_Lx, vy_Rx)  </span><span style="color: rgb(106, 153, 85);"># for partial_x (vy vx)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    F_vy_y </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> rusanov_flux(vy_Ly, vy_Ry, vy_Ly, vy_Ry)  </span><span style="color: rgb(106, 153, 85);"># for partial_y (vy^2)</span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    </span></span>
<span class="line"><span style="color: rgb(230, 230, 230);">    div_F_vy_x </span><span style="color: rgb(212, 212, 212);">=</span><span style="color: rgb(230, 230, 230);"> div_flux(F_vy_x, </span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(230, 230, 230);">, dx)</span></span>
