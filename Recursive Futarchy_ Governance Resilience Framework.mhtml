def estimate_Sigma_q(dq_list):
    """
    Robust proxy for sub-Gaussian parameter via median absolute deviation.
    """
    norms = np.linalg.norm(np.stack(dq_list), axis=1)
    mad = np.median(np.abs(norms - np.median(norms))) + 1e-12
    # For Gaussian, sigma ‚âà 1.4826 * MAD
    return 1.4826 * mad

