def estimate_sigma_signal(loglik_funcs, xpi_path):
    """
    Use residuals of ‚Ñì along a reference path as proxy; robust scale via MAD.
    """
    vals = []
    for ll in loglik_funcs:
        for xpi in xpi_path:
            vals.append(ll(xpi))
    vals = np.array(vals)
    res = vals - np.mean(vals)
    mad = np.median(np.abs(res - np.median(res))) + 1e-12
    return (1.4826 * mad)**2  # variance proxy

