def estimate_LV(contract_Lipschitzes, weights):
    # If V = sum_j w_j c_j and |c_j - c'_j| ‚â§ L_j * d, then L_V ‚â§ ||w||_1 * max_j L_j
    return np.sum(np.abs(weights)) * np.max(contract_Lipschitzes)

