# ----------------------------
def estimate_sigma_lambda2(lambda_V, LV_hat, XiV2_hat, lambda_y, sigma_signal2_hat):
    return (lambda_V**2)*(LV_hat**2)*XiV2_hat + (lambda_y**2)*sigma_signal2_hat

