# ----------------------------
# 1) Depth lower bound (Œ∫ÃÇ): Hessian min eigenvalue
# ----------------------------
def estimate_kappa(cost, price, q_samples, eps=1e-4):
    """
    Finite-diff Hessian of C around sample q's; return lower envelope of Œª_min.
    If using LMSR with liquidity b, you may set kappa = 1/b directly.
    """
    def hessian_at(q):
        m = q.size
        H = np.zeros((m, m))
        for i in range(m):
            e_i = np.zeros(m); e_i[i] = 1.0
            # symmetric finite diff on gradient (price) for column i
            g_plus  = price(q + eps*e_i)
            g_minus = price(q - eps*e_i)
            H[:, i] = (g_plus - g_minus) / (2*eps)
        return H

    lambdas = []
    for q in q_samples:
        H = hessian_at(q)
        # ensure symmetry numerically
        H = 0.5*(H + H.T)
        lam_min = np.linalg.eigvalsh(H).min()
        lambdas.append(lam_min)
    kappa_hat = np.percentile(lambdas, 5)  # conservative lower envelope
    return max(kappa_hat, 1e-12)

# ----------------------------
# 2) Impact Lipschitz (L_IÃÇ): ||Œîp|| &lt;= L_I ||Œîq||
# ----------------------------
def estimate_LI(dq_list, dp_list, quantile=0.95):
    """
    Use slope of the upper envelope of (||Œîq||, ||Œîp||) pairs.
    """
    X = np.linalg.norm(np.stack(dq_list), axis=1) + 1e-12
    Y = np.linalg.norm(np.stack(dp_list), axis=1)
    # robust slope via quantile of pointwise slopes
    slopes = Y / X
    return np.quantile(slopes, quantile)

# ----------------------------
# 3) Spread cap (sÃÑ): effective spread percentile
# ----------------------------
def estimate_spread_cap(mid_list, exec_price_list, quantile=0.90):
    spreads = np.abs(np.array(exec_price_list) - np.array(mid_list))
    return np.quantile(spreads, quantile)

# ----------------------------
# 4) Signal Lipschitz (L_‚ÑìÃÇ): max gradient norm of ‚Ñì wrt (x,pi)
# ----------------------------
def estimate_Ll(loglik_funcs, xpi_samples, dist, eps=1e-3, dirs=16):
    """
    For each y_t (loglik_func), estimate max directional derivative over a small sphere
    around sampled (x,pi). Users must provide local geodesic step 'step(xpi, u, eps)'.
    """
    def step(xpi, direction_unit, eps):
        # user: implement manifold-aware step; here we pretend Euclidean
        return xpi + eps*direction_unit

    grads = []
    for ll in loglik_funcs:
        for xpi in xpi_samples:
            # sample random unit directions
            D = np.random.normal(size=(dirs, xpi.size))
            D /= (np.linalg.norm(D, axis=1, keepdims=True) + 1e-12)
            g_norms = []
            f0 = ll(xpi)
            for u in D:
                xpi_p = step(xpi, u, eps)
                xpi_m = step(xpi, -u, eps)
                fp = ll(xpi_p); fm = ll(xpi_m)
                deriv = (fp - fm) / (2*eps)
                g_norms.append(abs(deriv))  # directional derivative magnitude
            grads.append(max(g_norms))
    return np.quantile(grads, 0.95)

